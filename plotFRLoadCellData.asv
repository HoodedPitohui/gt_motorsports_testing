function [] = plotFRLoadCellData(time, frData, min, max, speed, rowNum, index)
    figure;
    subplot(2, rowNum, index);
    plot(time(min: max), frData(min:max));
    xlabel('time (s)');
    ylabel('load (lbs)');
    title(['Front Load Cell Data (lbs) vs. Time (s) from t = ', ...
        'num2str(time(min)), '-', num2str(time(max))]);
    subplot(2, rowNum, index + 1);
    plotSpeedData(time, speed, min, max);
end